{
  "crons": [
    {
      "path": "/api/cron/daily-update",
      "schedule": "0 15 * * *"
    }
  ],
  "functions": {
    "app/api/cron/daily-update/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/cron/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
        }
      ]
    }
  ]
}

